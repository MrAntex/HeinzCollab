			setPixel(-2,1,"black");
			setPixel(-2,0,"black");
			setPixel(-1,0,"black");
			setPixel(1,0,"black");
			setPixel(2,0,"black");
			setPixel(2,-1,"black");
			setPixel(2,-2,"black");
			a=1;
		}
		sleep(200);
		n++;
	int prex=x;
	int prey=y;
	setPixel(x,y,"white");
	

	
   }
   println("Essais : " + n);
 }